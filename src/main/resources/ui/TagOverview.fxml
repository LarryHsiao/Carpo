<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?><?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
    xmlns:fx="http://javafx.com/fxml"
    fx:id="root"
    prefWidth="640"
    prefHeight="480"
    fx:controller="com.silverhetch.carpo.javafx.TagOverviewView">

    <TextField fx:id="tagName"
        editable="false" />

    <fx:include source="FileList.fxml"
        VBox.vgrow="ALWAYS"
        fx:id="fileList" />

</VBox>
